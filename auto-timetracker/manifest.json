{
    "manifest_version": 3,
    "name": "Timed Auto Clicker",
    "version": "1.2",
    "description": "Enter minutes to auto click and key press for that time.",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Set Duration"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  